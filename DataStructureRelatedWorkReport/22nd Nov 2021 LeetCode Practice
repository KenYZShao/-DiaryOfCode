class Solution():
   def twoSum(self, nums, target):
      """
      :type nums: List[int]
      :type target: int
      :rtype: List[int]
      """
      for i in range(len(nums)):
            num=nums[i]
            for j in range(i+1, len(nums)):
                s = nums[i] + nums[j]
                if s == target:
                    return [i,j]
                
##For TwoSum Problem https://leetcode.com/problems/two-sum/
